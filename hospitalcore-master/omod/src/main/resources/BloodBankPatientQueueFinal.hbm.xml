<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="org.openmrs.module.hospitalcore.model">
        <class name="BloodBankPatientFinal" table="bb_patient_info_final" batch-size="500">       
	<id name="id" type="int" column="id" unsaved-value="0"><generator class="native" /></id>  
        <property name="patientId" column="patient_id" />
        <property name="patientName" column="patient_name" />
        <property name="age" column="age" />
        <property name="gender" column="gender" />
        <property name="aboCell" column="abo_cell " />
        <property name="bloodGroup" column="blood_group" />   
        <property name="result" column="result" />
        <property name="identifier" column="patient_identifier" />
        <property name="bbPatientInfoId" column="bb_patient_info_id" />
        <property name="dateAccepted" column="date_accepted" />
                       	
	</class>
</hibernate-mapping>
